    "@trpc/next": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "geist": "^1.3.0",
    "lucide-react": "^0.330.0",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sonner": "^1.4.0",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
